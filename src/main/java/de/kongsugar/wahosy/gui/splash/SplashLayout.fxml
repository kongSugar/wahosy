<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/null"
            fx:controller="de.kongsugar.wahosy.gui.splash.SplashController">
    <children>
        <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                    AnchorPane.topAnchor="0.0">
            <center>
                <ProgressBar fx:id="progBar" prefWidth="300.0" progress="0.0" BorderPane.alignment="CENTER"/>
            </center>
            <top>
                <Label fx:id="countdown" text="Label" BorderPane.alignment="CENTER"/>
            </top>
        </BorderPane>
    </children>
</AnchorPane>
